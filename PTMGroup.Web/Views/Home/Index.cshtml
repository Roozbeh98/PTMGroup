<header>
    <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
        <div class="carousel-inner">
            <div class="heading">
                <div class="container">
                    <div class="row">
                        <div class="col-12">
                            <div class="float-left">
                                <a href="@Url.Action("Index")" class="logo text-hide">
                                    PTM Group
                                    <img src="~/Content/img/DENTAL CLINIC.png" alt="Website logo">
                                </a>
                            </div>
                            <div class="float-right">
                                <a href="@Url.Content("https://localhost:44322/Account/Login")" class="btn btn-info">Login / Register</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            @{ bool first = true; }
            @foreach (var item in Model.Sliders)
            {
                if (first)
                {
                    <div class="carousel-item active" style="background-image: url(@Url.Content("https://localhost:44322/FileManager/Download/?Key=" + item.Slider_DocumentGuid))">
                        <div class="container h-100">
                            <div class="content animated fadeInUp">
                                <h2>@item.Slider_Tital</h2>
                                <h4>@item.Slider_Subject</h4>
                                <span class="seperator"></span>
                                <p>@item.Slider_Description</p>
                            </div>
                        </div>
                    </div>
                    first = false;
                }
                else
                {
                    <div class="carousel-item" style="background-image: url(@Url.Content("https://localhost:44322/FileManager/Download/?Key=" + item.Slider_DocumentGuid))">
                        <div class="container h-100">
                            <div class="content animated fadeInUp">
                                <h2>@item.Slider_Tital</h2>
                                <h4>@item.Slider_Subject</h4>
                                <span class="seperator"></span>
                                <p>@item.Slider_Description</p>
                            </div>
                        </div>
                    </div>
                }
            }
        </div>
        <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>
    <nav class="container">
        <div class="row">
            <a href="#aboutUs" class="nav-item col-12 col-sm-6 col-md-4 col-lg-4 col-xl-2">
                <div class="image"></div>
                <h4>About Us</h4>
            </a>
            <a href="#workshops" class="nav-item col-12 col-sm-6 col-md-4 col-lg-4 col-xl-2">
                <div class="image"></div>
                <h4>Workshops</h4>
            </a>
            <a href="#products" class="nav-item col-12 col-sm-6 col-md-4 col-lg-4 col-xl-2">
                <div class="image"></div>
                <h4>Products</h4>
            </a>
