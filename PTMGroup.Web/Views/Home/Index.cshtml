<section id="contactUs">
    @*<div id="map"></div>*@
    <div class="container">
        <div class="contact-box">
            <form class="contact-form needs-validation" novalidate>
                <div class="form-group">
                    <label for="nameInput">Your Name</label>
                    <input type="text" class="form-control" id="nameInput" required />
                    <div class="invalid-feedback">
                        The field is required
                    </div>
                </div>
                <div class="form-group">
                    <label for="emailInput">Your Email</label>
                    <input type="email" class="form-control" id="emailInput" required />
                    <div class="invalid-feedback">
                        The field is required
                    </div>
                </div>
                <div class="form-group">
                    <label for="subjectInput">Subject</label>
                    <input type="text" class="form-control" id="subjectInput" />
                </div>
                <div class="form-group">
                    <label for="messageInput">Message</label>
                    <textarea type="text" class="form-control" id="messageInput" rows="6"></textarea>
                </div>
                <button class="btn btn-success" type="submit">send</button>
            </form>

            <div class="contact-desc">
                <h4 class="contact-desc-title">Contact Info & Details</h4>
                <p class="contact-desc-desc">
                    Premium WordPress Theme mainly Medical Oriented but so flexible that lets you build various layouts for any “Health & Beauty” related business!
                </p>
                <div class="contact-desc-workhours">
                    <b>Working hours:</b>
                    <span> 9am – 5pm on weekdays</span>
                </div>
                <p class="contact-desc-phone">(+30) 210 1234567</p>
                <p class="contact-desc-email">info@plethomedicalclinic.com</p>
                <p class="contact-desc-address">
                    79 Folsom Ave, San Francisco, CA 94107
                    <a href="#">get directions</a>
                </p>

                <h4 class="contact-desc-subtitle">Appointment Request</h4>
                <p class="contact-desc-subdesc">
                    If you wish to book an appointment with a doctor, it is best that you visit the <a href="#">Appointment Booking Page</a> directly.
                </p>
            </div>
        </div>
    </div>
</section>
